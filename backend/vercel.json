{
  "version": 2,
  "builds": [
    {
      "src": "src/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "50mb",
        "includeFiles": "src/**/*.py"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.py"
    }
  ]
}